#include <iostream>
#include <vector>

using namespace std;

bool subarraySumExists(vector<int>& nums, int target) {
    int start = 0, sum = 0;

    for (int end = 0; end < nums.size(); end++) {
        sum += nums[end];  // Expand window

        // Shrink window if sum exceeds target
        while (sum > target && start <= end) {
            sum -= nums[start];
            start++;
        }

        if (sum == target) return true;  // Found subarray
    }

    return false;  // No subarray found
}

int main() {
    vector<int> nums = {1, 4, 20, 3, 10, 5};
    int target = 33;

    if (subarraySumExists(nums, target))
        cout << "Subarray with sum " << target << " exists." << endl;
    else
        cout << "No subarray with sum " << target << " found." << endl;

    return 0;
}
